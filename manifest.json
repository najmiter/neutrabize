{
    "manifest_version": 3,
    "name": "neutrabize",
    "version": "3.01",
    "description": "Your own customisable Chrome new tab and home page",
    "permissions": ["geolocation", "topSites", "favicon"],

    "icons": {
        "32": "./neutrabize-128.png",
        "64": "./neutrabize-128.png",
        "128": "./neutrabize-128.png"
    },

    "chrome_url_overrides": {
        "newtab": "index.html"
    }
}
