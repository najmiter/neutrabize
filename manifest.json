{
    "manifest_version": 3,
    "name": "neutrabize",
    "version": "2.0",
    "description": "Your own customisable Chrome new tab and home page",
    "permissions": ["geolocation"],

    "icons": {
        "32": "imgs/neutrabize-128.png",
        "64": "imgs/neutrabize-128.png",
        "128": "imgs/neutrabize-128.png"
    },

    "chrome_url_overrides": {
        "newtab": "index.html"
    }
}
